<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dam2.collecardrmb">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".actividad.SettingsActivity" android:noHistory="true"></activity>
        <activity android:name=".actividad.AppInformationActivity" android:noHistory="true"/>
        <activity android:name=".actividad.MenuActivity" />
        <activity android:name=".actividad.LoginActivity" android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".actividad.RegisterActivity" />
        <activity android:name=".actividad.MainActivity" />
    </application>

</manifest>